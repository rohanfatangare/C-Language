//PROGRAMME FOR FINDING THE GREATER NUMBER BETWEEN TWO NUMBERS//
#include<stdio.h>
main()
{
	int no1,no2;
	printf("Enter two numbers\n");
	scanf("%d %d",&no1,&no2);
		if (no1==no2)
			printf("Both numbers are equal");
		else if (no1 < no2)
			printf("Greater number is %d\n",no2);
		else
			printf("Greater number is %d\n",no1);
}
